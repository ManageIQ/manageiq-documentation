<section id="Management_System_Folders_ems_folder" remap="TID_15688">
	<title>Management System Folders (ems_folder)</title>
	<para />
	<informaltable frame="all">
		<tgroup cols="2">
			<thead>
				<row>
					<entry>
						<para>
							Method
						</para>
					</entry>
					<entry>
						<para>
							Use
						</para>
					</entry>
				</row>
			</thead>
			<tbody>
				<row>
					<entry>
						<para>
							hosts
						</para>
					</entry>
